{{!
    @template local_musi/dashboard

    Classes required for JS:
    * none

    Data attributes required for JS:
    * data-region
}}


{{#item}}
    {{#link}}
        <a href="{{.}}">
    {{/link}}
    <div class="{{key}}">
        {{#key}}
            <span class="key">
                {{key}}:
            <span>
        {{/key}}
        {{#value}}
            </span class="value">
                {{value}}
            </span>
        {{/value}}
    </div>
    {{#link}}
        </a>
    {{/link}}
{{/item}}